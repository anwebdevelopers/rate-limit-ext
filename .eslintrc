{
    // "extends": ["eslint:recommended", "plugin:node/recommended"],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    // "extends": ["eslint:recommended", "plugin:node/recommended"],
    "rules": {
      "no-unused-vars": 2,
      "no-undef": 2
    },
    "parserOptions": {  
      "ecmaVersion": 2018,
      "ecmaFeatures": {
        "modules": true,
        "spread": true,
        "restParams": true,
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    }
  }
  